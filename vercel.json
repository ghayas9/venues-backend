{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "your_project_name.settings",
      "SECRET_KEY": "your-secret-key",
      "DEBUG": "False"
    }
  }
  